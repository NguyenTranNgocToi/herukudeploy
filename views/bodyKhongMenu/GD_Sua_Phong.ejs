<div class="form-group col-xs-4 col-sm-4 col-md-4 col-lg-4" style="margin-left: 30%; border-style: ridge;">
    <h2 style="text-align: center;font-weight: bold;">Sửa Thông Tin Phòng</h2>   
            <div>
                <hr>
            </div>
    <form action="/quanly/cnphong/suatt" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <input type="hidden" class="form-control" name="maPhong" value="<%=phong.maPhong%>">
        </div>
        <div class="form-group">
            <label>Tên loại phòng:</label>
            <select class="form-control" name="tenlph" required>
                <option selected value="defaut">
                    Mặc định
                </option>
                <%for(let i=0;i < dsTenLP.length;i++){%>
                    <option value="<%=dsTenLP[i].tenLoaiPhong%>">
                        <%=dsTenLP[i].tenLoaiPhong%>
                    </option>
                    <%}%>
            </select>
        </div>
        <button type="submit" class="btn btn-success" style="width: 100%;height: 40px;border-radius: 20px;">Lưu</button>
        <div>
            <br>
        </div>
    </form>
    <button onclick="BacktoHome()" class="btn btn-primary"
            style="width: 100%;margin-bottom: 5px;height: 40px;border-radius: 20px;">Quay lại</button>
</div>
<script>
    function BacktoHome() {
        location.replace('/quanly/cnphong')
    }           
</script>
</div>