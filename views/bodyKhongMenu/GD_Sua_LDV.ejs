<div class="container">
    <div class="form-group col-xs-5 col-sm-5 col-md-5 col-lg-5" style="margin-left: 30%; border-style: ridge;">
        <h2 style="text-align: center;font-weight: bold;">Sửa Thông Tin Loại Dịch Vụ</h2>   
            <div>
                <hr>
            </div>
        <form action="/quanly/cnloaidv/suatt" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <input type="hidden" class="form-control" name="maLoaiDichVu" value="<%=ldv.maLoaiDichVu%>">
            </div>
            <div class="form-group">
                <label for="Tenldv">Tên loại dịch vụ:</label>
                <input value="<%=ldv.tenLoaiDichVu%>" type="text" class="form-control" placeholder="Tên loại dịch vụ" name="tenLoaiDichVu">
            </div>
            <button type="submit" class="btn btn-success" style="width: 100%;height: 40px;border-radius: 20px;">Lưu</button>
            <div>
                <br>
            </div>
        </form>
        <button onclick="BacktoHome()" class="btn btn-primary"
            style="width: 100%;margin-bottom: 5px;height: 40px;border-radius: 20px;">Quay lại</button>
    </div>
    <script>
        function BacktoHome() {
            location.replace('/quanly/cnloaidv')
        }           
    </script>
</div>